<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>TazUO_Launcher</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="menu.png" />
    <None Remove="taz2.png" />
    <None Remove="Untitled_Artwork %283%29.png" />
    <None Remove="Untitled_Artwork %284%29.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignThemes" Version="4.9.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="menu.png" />
    <Resource Include="taz2.png" />
    <Resource Include="Untitled_Artwork (3).png" />
    <Resource Include="Untitled_Artwork (4).png" />
  </ItemGroup>

</Project>
